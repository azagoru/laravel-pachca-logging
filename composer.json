{
    "name": "asavenkov/laravel-pachca-logger",
    "description": "Logging to a Pachca in Laravel",
    "keywords": [
        "laravel",
        "logger",
        "logging",
        "pachca"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Savenkov",
            "email": "a@savenkov.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~8.0|~8.1|~8.2",
        "monolog/monolog": "~1.12|^2.0|^3.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/contracts": "~7.0|~8.0|~9.0|~10.0",
        "illuminate/support": "~7.0|~8.0|~9.0|~10.0",
        "illuminate/queue": "~7.0|~8.0|~9.0|~10.0"
    },
    "autoload": {
        "psr-4": {
            "SavenkovDev\\PachcaLogger\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
